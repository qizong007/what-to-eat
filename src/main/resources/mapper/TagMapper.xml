<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fzufood.repository.TagMapper">
    
    <select id="listTags" resultType="Tag">
        select * from tag
    </select>

    <select id="getTagById" parameterType="java.lang.Integer" resultType="Tag">
        select * from tag where tag_id = #{id}
    </select>

    
</mapper>